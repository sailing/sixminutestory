	<div class=logo>
	<span id="logo"><%= image_tag "logo.png" %></span>
	</div>
	<div id="header_links">
	<% if !current_user %>
	  	<%= link_to "Register", new_account_path %> |
	  	<%= link_to "Log In", new_user_session_path %>
	| <%= pluralize User.logged_in.count, "user" %> currently logged in <!-- This based on last_request_at, if they were active < 10 minutes ago they are logged in -->
	<% else %>
		<%= link_to "Logout", user_session_path, :method => :delete, :confirm => "Are you sure you want to logout?" %> |
	  	<%= link_to "My Account", account_path %> |
		<%= link_to "Shares", shares_path %> |
		<%= link_to "New Share", new_share_path %>
	<% end %>
	
	</div>  
