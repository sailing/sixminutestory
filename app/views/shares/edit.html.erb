

<% if current_user.id == @share.user_id %>
	<% form_for(@share) do |f| %>
  		<%= f.error_messages %>
  		<%= render :partial => "form", :object => f %>
	<% end %>
<% else %>
	
	<div>
		<h2>You didn't create this share, so you can't edit it. <br /><br />How did you even get here?
		</h2>
	</div>
<% end %>